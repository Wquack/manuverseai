{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/manifest.json",
      "destination": "/manifest.json",
      "permanent": true
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico",
      "permanent": true
    }
  ]
}
